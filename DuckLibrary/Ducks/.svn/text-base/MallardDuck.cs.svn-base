using System;

namespace DuckLibrary.Ducks
{
    public class MallardDuck : Duck, IQuackable
    {
        public void Quack()
        {
            Console.WriteLine("Quack");
        }

        public override string SetLocation()
        {
            return "I'm going to the zoo!";
        }

        public string Name { get; set; }
    }
}